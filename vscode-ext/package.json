{
    "name": "fso-tables",
    "displayName": "FSO Tables",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "./out/extension.js",
    "activationEvents": [
        "onLanguage:fso-table"
    ],
    "contributes": {
        "languages": [
            {
                "id": "fso-table",
                "aliases": [
                    "FSO Table",
                    "fso-table"
                ],
                "extensions": [
                    ".tbl",
                    ".tbm",
                    ".fs2"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "fso-table",
                "scopeName": "source.fso-table",
                "path": "./syntaxes/fso-table.tmLanguage.json"
            }
        ],
        "commands": [{
            "command": "fso-tables.restart",
            "title": "Restart FSO Tables LSP"
        }]
    },
    "scripts": {
        "vscode:prepublish": "compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "dependencies": {
        "@types/node": "^15.3.0",
        "@types/vscode": "^1.56.0",
        "typescript": "^4.2.4",
        "vscode-languageclient": "^7.0.0"
    }
}
